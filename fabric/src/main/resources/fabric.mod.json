{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${project_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "${project_homepage}",
    "sources": "${project_issues}"
  },
  "license": "${mod_license}",
  "icon": "assets/${mod_id}/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "${project_group}.fabric.${fabric_entrypoint}"
    ]
  },
  "depends": {
    "fabric": "*",
    "minecraft": ">=${minecraft_version}"
  }
}